{
	"name": "ts-crawler",
	"version": "1.0.0",
	"description": "typescript crawler",
	"main": "index.js",
	"scripts": {
		"ts:dev": "ts-node ./src/index.ts",
		"dev:build": "tsc -w",
		"dev:start": "nodemon node ./build/index.js",
		"dev": "concurrently yarn:dev:*",
		"build": "tsc"
	},
	"nodemonConfig": {
		"ignore": [
			"output/*",
			"build/*"
		]
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"concurrently": "^5.3.0",
		"nodemon": "^2.0.4",
		"prettier": "^2.0.5",
		"ts-node": "^8.10.2",
		"typescript": "^3.9.7"
	},
	"dependencies": {
		"@types/cheerio": "^0.22.21",
		"@types/superagent": "^4.1.9",
		"cheerio": "^1.0.0-rc.3",
		"dayjs": "^1.8.34",
		"superagent": "^6.0.0"
	}
}
